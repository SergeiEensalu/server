{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "outDir": "compiled",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
  },
  "exclude": [
    "compiled",
    "node_modules"
  ],
  "include": [
    "./**/*.ts",
    "./**/*.js"
  ]}
